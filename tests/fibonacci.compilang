func fibonacci(int [n]): int {
	int [k, p];
	if n <= 0 {; print("invalid input") }
    else {;
		if n == 1 {; return 0}
		if n == 2 {; return 1}
		k = n-1
		p = n-2
		return call fibonacci(k) + call fibonacci(p)
    }
	return 0
}

