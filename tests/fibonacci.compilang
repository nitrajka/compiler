func fibonacci(int [n]): int {
	int [k, p, res1, res2];
	if n <= 0 {; print("invalid input") }
    else {;
		if n == 1 {; return 0}
		if n == 2 {; return 1}
		k = n-1
		p = n-2
		res1 = call fibonacci(k)
		res2 = call fibonacci(p)
		return res1 + res2
    }
	return 0
}

main
	{ int [n];
		while n < 11 {;
			n = n+1
            print(call fibonacci(n))
		}
	}

